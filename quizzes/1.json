{
    "author": "Matias Barrios",
    "description": "First Linux quiz for testing",
    "questions": [
        {
            "id": "q1",
            "mode": "single",
            "question": "Which of these commands is useful to see the amount of free RAM?",
            "options": [
                "ls -lh",
                "du -s",
                "df -h",
                "free -h"
            ],
            "answers": [
                "free -h"
            ]
        },
        {
            "id": "q2",
            "mode": "multi",
            "question": "Which of these are NOT a builtin command in Bash",
            "options": [
                "cd",
                "docker",
                "printf",
                "git"
            ],
            "answers": [
                "git",
                "docker"
            ]
        }
    ]
}